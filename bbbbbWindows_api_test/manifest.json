{
    "manifest_version": 2,
    "version": "0.0.0",
    "name": "bWindow secrets garden",

    "permissions": [
        "tabs",
        "<all_urls>", "sessions",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "notifications"


    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {

        "default_title": "windows test"
    },
    "web_accessible_resources": [
        "images/*"
    ]


}