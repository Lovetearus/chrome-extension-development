{
    "manifest_version": 2,
    "version": "0.0.0",
    "name": "beginner",
    "icons": {
        "16": "images/0.png",
        "48": "images/0.png",
        "128": "images/0.png",
        "256": "images/0.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>", "sessions"


    ],
    "background": {
        "js": "js/background.js"
    },
    "browser_action": {
        "default_icon": "images/0.png",
        "default_title": "replace all images on website",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "images/*"
    ]


}